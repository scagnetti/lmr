<div class="row">
	<%= render 'score', :score => @score_card.equity_ratio.score %>
	<div class="col-md-3">
		Eigenkapitalquote LJ
		<span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="> 25% | 25% - 15% | < 15%"></span>
	</div>
	<% if @score_card.equity_ratio.succeeded %>
		<div class="col-md-2">
			Value: <%= @score_card.equity_ratio.value %>%
		</div>
		<div class="col-md-2">
			Year: <%= @score_card.equity_ratio.last_year %>
		</div>
	<% else %>
		<%= render 'failed' %>
	<% end %>
</div>