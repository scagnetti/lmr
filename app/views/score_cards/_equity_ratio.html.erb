<div class="row">
	<%= render 'score', :score => @score_card.equity_ratio.score %>
	<div class="col-xs-11 col-md-3">
		<span class="glyphicon glyphicon-info-sign" data-toggle="tooltip" title="<%= resolve_info_for_equity_ratio(@score_card.share)%>"></span> Eigenkapitalquote LJ
	</div>
	<% if @score_card.equity_ratio.succeeded %>
		<div class="col-xs-6 col-md-2">
			Value: <%= @score_card.equity_ratio.value %>%
		</div>
		<div class="col-xs-6 col-md-2">
			Year: <%= @score_card.equity_ratio.last_year %>
		</div>
	<% else %>
		<%= render 'failed' %>
	<% end %>
</div>